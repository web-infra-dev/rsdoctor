{
  "name": "@rsdoctor/core",
  "version": "1.2.4-beta.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rsdoctor",
    "directory": "packages/core"
  },
  "license": "MIT",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "compiled"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./build-utils": {
      "types": "./dist/build-utils/build/index.d.ts",
      "import": "./dist/build-utils/build/index.js",
      "default": "./dist/build-utils/build/index.cjs"
    },
    "./plugins": {
      "types": "./dist/inner-plugins/index.d.ts",
      "import": "./dist/inner-plugins/index.js",
      "default": "./dist/inner-plugins/index.cjs"
    },
    "./rules": {
      "types": "./dist/rules/index.d.ts",
      "import": "./dist/rules/index.js",
      "default": "./dist/rules/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "default": "./dist/types/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "build-utils": [
        "./dist/build-utils/build/index.d.ts"
      ],
      "plugins": [
        "./dist/inner-plugins/index.d.ts"
      ],
      "rules": [
        "./dist/rules/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "npm run start",
    "build": "npm run prebundle && rslib build",
    "start": "rslib build -w",
    "test": "rstest run",
    "prebundle": "prebundle"
  },
  "dependencies": {
    "@rsdoctor/graph": "workspace:*",
    "@rsdoctor/sdk": "workspace:*",
    "@rsdoctor/types": "workspace:*",
    "@rsdoctor/utils": "workspace:*",
    "browserslist-load-config": "^1.0.1",
    "filesize": "^10.1.6",
    "fs-extra": "^11.1.1",
    "lodash-es": "^4.17.21",
    "semver": "^7.7.2",
    "source-map": "^0.7.6"
  },
  "devDependencies": {
    "@rsbuild/plugin-check-syntax": "1.4.0",
    "axios": "^1.12.0",
    "enhanced-resolve": "5.12.0",
    "@rspack/core": "1.5.2",
    "@scripts/test-helper": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.8.1",
    "@types/node-fetch": "^2.6.12",
    "@types/semver": "^7.7.0",
    "@types/tapable": "2.2.7",
    "babel-loader": "10.0.0",
    "prebundle": "1.4.2",
    "string-loader": "0.0.1",
    "ts-loader": "^9.5.4",
    "tslib": "2.8.1",
    "typescript": "^5.9.2",
    "webpack": "^5.97.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
