{
  "name": "@rsdoctor/core",
  "version": "0.4.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rsdoctor",
    "directory": "packages/core"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./build-utils": {
      "types": "./dist/build-utils/build/index.d.ts",
      "default": "./dist/build-utils/build/index.js"
    },
    "./common-utils": {
      "types": "./dist/build-utils/common/index.d.ts",
      "default": "./dist/build-utils/common/index.js"
    },
    "./plugins": {
      "types": "./dist/inner-plugins/index.d.ts",
      "default": "./dist/inner-plugins/index.js"
    },
    "./rules": {
      "types": "./dist/rules/index.d.ts",
      "default": "./dist/rules/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "build-utils": [
        "./dist/build-utils/build/index.d.ts"
      ],
      "common-utils": [
        "./dist/build-utils/common/index.d.ts"
      ],
      "plugins": [
        "./dist/inner-plugins/index.d.ts"
      ],
      "rules": [
        "./dist/rules/index.d.ts"
      ],
      "types": [
        "./dist/types/types.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "npm run start",
    "build": "modern build",
    "start": "modern build -w",
    "test": "vitest run"
  },
  "dependencies": {
    "@rsdoctor/graph": "workspace:*",
    "@rsdoctor/sdk": "workspace:*",
    "@rsdoctor/types": "workspace:*",
    "@rsdoctor/utils": "workspace:*",
    "axios": "^1.7.7",
    "enhanced-resolve": "5.12.0",
    "filesize": "^10.1.6",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "path-browserify": "1.0.1",
    "semver": "^7.6.3",
    "source-map": "^0.7.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "devDependencies": {
    "@rspack/core": "1.0.14",
    "@scripts/test-helper": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.13",
    "@types/node": "^16",
    "@types/node-fetch": "^2.6.11",
    "@types/path-browserify": "1.0.3",
    "@types/semver": "^7.5.8",
    "@types/tapable": "2.2.7",
    "babel-loader": "9.1.3",
    "string-loader": "0.0.1",
    "ts-loader": "^9.5.1",
    "tslib": "2.7.0",
    "typescript": "^5.2.2",
    "webpack": "^5.95.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
