  "devDependencies": {
    "@types/babel__code-frame": "7.0.6",
    "@types/connect": "3.4.38",
    "@types/deep-eql": "4.0.2",
    "@types/envinfo": "7.8.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.8.1",
